%p#notice= notice

%p
  %b Parkname:
  = @park.parkname
%p
  %b Location:
  = @park.location
%p
  %b Info:
  = @park.info

= link_to 'Edit', edit_park_path(@park)
\|
= link_to 'Back', parks_path

%table#microposts
  %thead
    %tr
      %th Park
      %th Content
      %th User
  %tbody
    - @microposts.each do |post|
      %tr
        %td= post.park_id
        %td= post.content
        %td= post.user_id

= link_to 'Add new post', new_micropost_path
